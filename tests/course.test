<?php/** * @file * Tests for the Course module */class CourseTestCase extends DrupalWebTestCase{  public static function getInfo() {    return array(      'name' => 'Course module functionality',      'description' => 'Tests hooks in the Course module.',      'group' => 'Course',    );  }  public function setUp(){    parent::setUp('course');  }  public function testMenu(){    $items = module_invoke('course', 'menu');    $this->assertEqual(1, count($items), t('One item in menu.'));    $this->assertIdentical($items['admin/config/course']['title'],    $items['Course']['title'], t('Titles are identical.'));  }}